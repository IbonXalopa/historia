{
  "database": {
    "user": "historia",
    "password": "historia",
    "host": "127.0.0.1",
    "main_database":"histora_test_db",
    "user_database_name_prefix":"historia_test",
    "raise_on_warnings": False
  },
  "server": {
    "port": 4443,
    "cert_file": "../ssl_cert_/historia.pem"
  },
  "logging":{
    "version":1,
    "handlers":{
      "console": {
          "class": "logging.StreamHandler",
          "level": "DEBUG",
          "formatter": "default",
          "stream": "sys.stderr"
      }
    },
    "root": {
      "level":"NOTSET",
      "handlers" : ["console"]
    },
    "formatters": {
      "default": "format=%(asctime)s: %(name)s - %(levelname)s - %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    },
    "filters": {},
    "loggers": {
      "historia.ctrl":{
        "level":"ERROR",
        "handlers" : ["console"]
      },
      "historia.db":{
        "level":"ERROR",
        "handlers" : ["console"]
      }
    },
    "incremental": false,
    "disable_existing_loggers": false
  }
}